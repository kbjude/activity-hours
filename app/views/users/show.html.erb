<h1> User Profile </h1>

<h3> All my hours</h3>
<p>
<% @user_hours.each do |uh| %>
    <%= link_to uh.hours, hour_path(uh) %>hrs
    <%= link_to uh.description, hour_path(uh)%>
<% end %>
</p>

<h3> All my external hours </h3>

<h3> All Groups</h3>
    <% @user_groups.each do |group| %>
        <%= group.name %>
    <% end %>